{
    "name": "NimoTV",
    "description": "description",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.nimo.tv/*"
            ],
            "css": [
                "./content_scripts/main.css"
            ],
            "js": [
                "./content_scripts/main.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "./action/popup.html",
        "default_icon": {
            "16": "./assets/logo.png",
            "32": "./assets/logo.png",
            "48": "./assets/logo.png",
            "128": "./assets/logo.png"
        }
    },
    "icons": {
        "16": "./assets/logo.png",
        "32": "./assets/logo.png",
        "48": "./assets/logo.png",
        "128": "./assets/logo.png"
    }
}